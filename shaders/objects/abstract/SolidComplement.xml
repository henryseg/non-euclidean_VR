<?xml version="1.0" encoding="UTF-8"?>
<library>
    <background>
    </background>
    <items>
        <solid class="solidComplement">
            <shader type="declare">
                Solid {{name}};
            </shader>
            <shader type="setup">
                {{name}} = {{toGLSL}};
            </shader>
            <shader type="sdf">
                return - {{child.name}}SDF(v);
            </shader>
            <shader type="gradient">
                return negate({{child.name}}Grad(v));
            </shader>
        </solid>
    </items>
</library>