<?xml version="1.0" encoding="UTF-8"?>
<subgroup>
    <background>
        const float cubeHalfWidth = 0.5;
    </background>
    <teleports>
        <teleport><![CDATA[
            return p.coords.x > cubeHalfWidth;
        ]]></teleport>
        <teleport><![CDATA[
            return p.coords.x < - cubeHalfWidth;
            ]]></teleport>
        <teleport><![CDATA[
            return p.coords.y > cubeHalfWidth;
            ]]></teleport>
        <teleport><![CDATA[
            return p.coords.y < - cubeHalfWidth;
            ]]></teleport>
        <teleport><![CDATA[
            return p.coords.z > cubeHalfWidth;
            ]]></teleport>
        <teleport><![CDATA[
            return p.coords.z < - cubeHalfWidth;
            ]]></teleport>
    </teleports>
</subgroup>