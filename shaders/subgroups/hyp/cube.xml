<?xml version="1.0" encoding="UTF-8"?>
<subgroup>
    <background>
        const float cubeHalfWidth = 0.57735026;
    </background>
    <teleports>
        <teleport><![CDATA[
            return p.coords.x > cubeHalfWidth*p.coords.w;
        ]]>
        </teleport>
        <teleport><![CDATA[
            return p.coords.x  < - cubeHalfWidth*p.coords.w;
        ]]>
        </teleport>
        <teleport><![CDATA[
            return p.coords.y  > cubeHalfWidth*p.coords.w;
        ]]>
        </teleport>
        <teleport><![CDATA[
            return p.coords.y  < - cubeHalfWidth*p.coords.w;
        ]]>
        </teleport>
        <teleport><![CDATA[
            return p.coords.z  > cubeHalfWidth*p.coords.w;
        ]]>
        </teleport>
        <teleport><![CDATA[
            return p.coords.z  < - cubeHalfWidth*p.coords.w;
        ]]>
        </teleport>
    </teleports>
</subgroup>
