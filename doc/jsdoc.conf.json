{
    "plugins": [
        "plugins/markdown"
    ],
    "markdown": {
        "tags": ["todo"]
    },
    "source": {
        "include": [
            "../js/geometry/foundations",
            "../js/geometry/items",
            "../js/"
        ],
        "exclude": [
            "../js/lib",
            "../js/unittest"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },

    "opts" : {
        "destination": "./jsdoc/"
    }
}
