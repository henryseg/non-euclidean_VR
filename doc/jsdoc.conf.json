{
    "plugins": [
        "plugins/markdown"
    ],

    "source": {
        "include": [
            "../js/geometry/",
            "../js/scene/",
            "../js/"
        ],
        "exclude": [
            "../js/lib",
            "../js/unittest"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },

    "opts" : {
        "destination": "./jsdoc/"
    }
}
